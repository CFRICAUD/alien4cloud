{
  "id": "topologyId",
  "node_templates": {
    "tomcat_server": {
      "type": "fr.fastconnect.Tomcat",
      "properties": {
      },
      "relationships": {
        "hosted_on": {
          "type": "tosca.relationships.HostedOn",
          "target": "tomcat_compute"
        },
        "connected_to": {
          "type": "tosca.relationships.ConnectsTo",
          "target": "mysql"
        },
        "depends_on": {
          "type": "tosca.relationships.DependsOn",
          "target": "java"
        }
      }
    },
    "java": {
      "type": "fr.fastconnect.Java",
      "properties": {
        "version": "1.7.24"
      },
      "relationships": {
        "hosted_on": {
          "type": "tosca.relationships.HostedOn",
          "target": "tomcat_compute"
        }
      }
    },
    "tomcat_compute": {
      "type": "tosca.nodes.Compute",
      "properties": {
        "num_cpus": 2,
        "mem_size": 512,
        "os_arch": "x86_64",
        "os_type": "linux"
      }
    },
    "mysql": {
      "type": "fr.fastconnect.MySQL",
      "properties": {
        "version": "5.12"
      },
      "relationships": {
        "hosted_on": {
          "type": "tosca.relationships.HostedOn",
          "target": "mysql_compute"
        }
      }
    },
    "mysql_compute": {
      "type": "tosca.nodes.Compute",
      "properties": {
        "num_cpus": 2,
        "mem_size": 512,
        "os_arch": "x86_64",
        "os_type": "linux"
      }
    }
  }
}